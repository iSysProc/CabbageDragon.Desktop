{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "allowJs": true,
        "jsx": "react-jsx",
        "jsxImportSource": "@emotion/react",
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "baseUrl": "./src",
        "forceConsistentCasingInFileNames": true,
        "noEmit": true
    },
    "exclude": [
        "./dist/**/*"
    ]
}